    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-table": "^6.7.4",
    "redux": "^3.7.2",
